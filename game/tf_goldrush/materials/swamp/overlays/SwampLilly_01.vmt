"UnlitGeneric"
{
	"$basetexture" "swamp/overlays/SwampLilly_01"
	"$translucent" 1
	"$vertexcolor" "1"
	"$vertexalpha" "1"
	"$decal" 1
	"%keywords" "tf"

 "$temp" "[0 0]"
 "$curr" 0.0
 "$curr2" 0.0

 "Proxies"
 {
		"Sine"
		{
			"sineperiod" "8"
			"sinemin" -0.015
			"sinemax" 0.005
			"resultVar" "$curr"
		}
		"Sine"
		{
			"sineperiod" "5"
			"sinemin" 0.003
			"sinemax" -0.015
			"resultVar" "$curr2"
		}

		"Equals"
		{
			"srcVar1" "$curr2"
			"resultVar" "$temp[0]"
		}
		"Equals"
		{
			"srcVar1" "$curr"
			"resultVar" "$temp[1]"
		}

 
		"TextureTransform"
		{
			"translateVar" "$temp"
			"resultVar" "$basetexturetransform"
		}
	}  
}
